{
  "name": "akeeldataformater",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "main": "public/electron.js",
  "build": {
    "appId": "com.laithmohammedsaker.akeeldataformater",
    "win": {
      "target": "nsis"
    }
  },
  "dependencies": {
    "@popperjs/core": "^2.11.4",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "bootstrap": "^5.1.3",
    "concurrently": "^6.3.0",
    "csv-parser": "^3.0.0",
    "docx": "^7.3.0",
    "electron-is-dev": "^2.0.0",
    "file-saver": "^2.0.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.8",
    "react-router-dom": "^6.2.2",
    "react-scripts": "4.0.3",
    "redux": "^4.1.2",
    "wait-on": "^6.0.0",
    "web-vitals": "^1.0.1",
    "xlsx": "^0.18.4"
  },
  "scripts": {
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "release": "react-scripts build && electron-builder --publish=never",
    "start": "concurrently \"SET BROWSER=none && npm run react-start\" \"wait-on http://localhost:3000 && electron .\"",
    "build": "react-scripts build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^13.2.1",
    "electron-builder": "^22.13.1"
  }
}
